{
   "compilerOptions": {
      "jsx": "react-jsx",
      "allowJs": true,
      "checkJs": true,
      "noEmit": true,
      "target": "ESNext", 
      "module": "ESNext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
   },
   "include": ["src/**/*"],
   "exclude": ["node_modules"]
}